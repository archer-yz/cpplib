@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Find required dependencies
find_dependency(fmt CONFIG REQUIRED)

# Include the targets file
include("${CMAKE_CURRENT_LIST_DIR}/cpplibTargets.cmake")

# Define convenience variables
set(CPPLIB_FOUND TRUE)
set(CPPLIB_VERSION "@PROJECT_VERSION@")

# Check if we have the expected targets
if(NOT TARGET cpplib::cpplib)
    set(CPPLIB_FOUND FALSE)
    set(CPPLIB_NOT_FOUND_MESSAGE "Expected target cpplib::cpplib not found")
endif()

check_required_components(cpplib)
